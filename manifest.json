{
    "manifest_version": 2,
    "name": "New Tab Bookmark",
    "version": "0.2.3",
    "description": "Replace New Tab page with a list of your bookmark.",
    "icons": {
    },
    "developer": {
        "name": "Koji Yamamoto"
    },
    "chrome_url_overrides": {
        "newtab": "html/index.html"
    },
    "permissions": [
        "bookmarks",
        "storage"
    ],
    "options_ui": {
        "page": "html/options.html",
        "browser_style": true
    }
}
